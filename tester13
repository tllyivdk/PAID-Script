local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()
local Window = Fluent:CreateWindow({
    Title = "Select Premium Script",
    SubTitle = "Created By AfxcaxiRoblox And My Best Friends",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- nhấn phím Ctrl để bật tắt UI
})
local Tabs = {
    Main = Window:AddTab({ Title = "Maru Hub", Icon = "calendar" }),
}

Tabs.Main:AddParagraph({
    Title = "Maru Hub",
    Content = "For Those Who Bought It"
})


Tabs.Main:AddButton({
    Title = "Maru Hub[Thai Version]",
    Description = "Thai Language Version",
    Callback = function()
        getgenv().Key = "MARU-PFVS-93X3K-1XX8-LG3EB-TAHA"
        getgenv().id = "919580300013604905"
        _G.Script_Language = "Thai"
        loadstring(game:HttpGet("https://raw.githubusercontent.com/xshiba/MaruBitkub/main/Mobile.lua"))()
    end
})

Tabs.Main:AddButton({
    Title = "Maru Hub[All Map]",
    Description = "Main Version, Custom Features",
    Callback = function()
        getgenv().Key = "MARU-PFVS-93X3K-1XX8-LG3EB-TAHA"
        getgenv().id = "919580300013604905"
        loadstring(game:HttpGet("https://raw.githubusercontent.com/xshiba/MaruBitkub/main/Mobile.lua"))()
    end
})
