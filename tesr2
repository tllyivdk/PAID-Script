local Mercury = loadstring(game:HttpGet("https://raw.githubusercontent.com/tllyivdk/PAID-Script/main/Mercury%20UI"))()

local GUI = Mercury:Create{
    Name = "Select Premium Script", --nhập name hub 
    Size = UDim2.fromOffset(580, 460),
    Theme = Mercury.Themes.Aqua,
    Link = "https://github.com/deeeity/mercury-lib"
}

--tạo tab
local Tab = GUI:Tab{
    Name = "Maru Hub", --đặt tên tab
    Icon = "rbxassetid://7733674820"
}

--Tạo Button
Tab:Button{
    Name = "Main Version", --Name Button
    Description = nil,
    Callback = function()
        getgenv().Key = "MARU-WBBSW-ZWBL0-6F8N-I80D5-RYFH"
        getgenv().id = "1003662302412820622"
        _G.Script_Language = "Thai"
        loadstring(game:HttpGet("https://raw.githubusercontent.com/xshiba/MaruBitkub/main/Mobile.lua"))()
        wait(.1)
        getgenv().Key = "Do You Think You Can Use ClipBoard To Get The Key Or ID Of Our Hacker? Never!"
        getgenv().id = "Do You Think You Can Use ClipBoard To Get The Key Or ID Of Our Hacker? Never!"  
    end
}

